{
  "extends": "airbnb",
  "env": { "browser": true },
  "settings": {
    "polyfills": ["fetch"]
  },
  "rules": {
    "react/forbid-prop-types": [0],
    "react/no-unused-prop-types": [0],
    "no-underscore-dangle":[0],
    "max-len":[0],
    "jsx-a11y/no-static-element-interactions":[0],
    "no-plusplus":[0],
    "no-return-assign": [0],
    "no-param-reassign":[0],
    "global-require":[0]
  }
}
